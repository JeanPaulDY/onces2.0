<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Onces20" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/amarillo_main</item>
        <item name="colorPrimaryVariant">@color/amarillo_main</item>
        <item name="colorOnPrimary">@color/verde_main</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/amarillo_main</item>
        <item name="colorSecondaryVariant">@color/verde_main</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.Onces20.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Onces20.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Onces20.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>