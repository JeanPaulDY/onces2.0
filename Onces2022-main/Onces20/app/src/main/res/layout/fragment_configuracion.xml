<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ui.fragments.ConfiguracionFragment">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/configuracion_fondo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginTop="170dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Dirección de envío"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"

        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:layout_marginTop="230dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Método de pago"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"

        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="120dp"
        android:layout_height="32dp"
        android:layout_marginTop="290dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Perfil"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"

        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/buttonnavigation"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/amarillo_main"
        android:layout_marginTop="670dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/menu_navigation_bar">
    </com.google.android.material.bottomnavigation.BottomNavigationView>
</androidx.constraintlayout.widget.ConstraintLayout>