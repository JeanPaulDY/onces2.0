<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ui.fragments.FacturaFragment">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/facturar_fondo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textCodNumF"
        android:layout_width="160dp"
        android:layout_height="32dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Codigo No"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.846"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <TextView
        android:id="@+id/textNameF"
        android:layout_width="250dp"
        android:layout_height="32dp"
        android:layout_marginTop="245dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="A nombre de quien"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.757"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <TextView
        android:id="@+id/textFormaPagoF"
        android:layout_width="180dp"
        android:layout_height="32dp"
        android:layout_marginTop="310dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Forma de pago"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <TextView
        android:id="@+id/textTotalProdF"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="375dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Total prod."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <TextView
        android:id="@+id/textTotalValorF"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:layout_marginTop="440dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Total prod."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

    <Button
        android:id="@+id/buttonFacturarF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="528dp"
        android:background="@drawable/borde_amarillo_fondo_naranja"
        android:fontFamily="@font/roboto_mono_medium_italic"
        android:text="Generar su factura"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20" />

</androidx.constraintlayout.widget.ConstraintLayout>